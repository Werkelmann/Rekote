<resources>

    <!-- Base application theme. -->
    <style caller="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item caller="colorPrimary">@color/colorPrimary</item>
        <item caller="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item caller="colorAccent">@color/colorAccent</item>
    </style>
    <style caller="AppTheme.NoActionBar">
        <item caller="windowActionBar">false</item>
        <item caller="windowNoTitle">true</item>
    </style>
    <style caller="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style caller="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>
